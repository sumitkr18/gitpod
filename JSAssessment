#Step 1: Create a variable to hold the number of NFTs. We'll use the "number" data type for thislet numberOfNFTs = 10;
let numberOfNFTs = 0;
#Step 2: Create an object inside the mintNFT function to hold the metadata for your NFTs. The metadata values will be passed as parameters, and when the NFT is ready, it will be stored in the variable created in step 1.
let nftMetadata = {};

function mintNFT(name, description, image) {
  // Create an NFT object with metadata
  const newNFT = {
    name: name,
    description: description,
    image: image
  };

  // Store the NFT in the variable and increment the count
  numberOfNFTs++;
  nftMetadata[numberOfNFTs] = newNFT;
}
#Step 3: Create a listNFTs function that will print all of the NFTs' metadata to the console.
function listNFTs() {
  for (let nftId in nftMetadata) {
    const nft = nftMetadata[nftId];
    console.log("Name: " + nft.name);
    console.log("Description: " + nft.description);
    console.log("Image: " + nft.image);
    console.log("-------------------");
  }
}
#Step 4: Implement the getTotalSupply function that returns the number of NFTs created.
function getTotalSupply() {
  return numberOfNFTs;
}
#Now you can use these functions to manage and display your NFTs. For example:
mintNFT("NFT 1", "This is the first NFT", "image1.jpg");
mintNFT("NFT 2", "This is the second NFT", "image2.jpg");

listNFTs(); // Prints the metadata of all NFTs
console.log("Total NFTs: " + getTotalSupply()); // Prints the total number of NFTs
